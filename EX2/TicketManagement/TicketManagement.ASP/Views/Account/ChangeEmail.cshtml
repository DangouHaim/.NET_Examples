@using TicketManagement.ASP.Views.Account;
@using LangUs = TicketManagement.ASP.Views.UserPage.Lang;


<h2>@Lang.ChangeEmail</h2>

@Html.ValidationSummary(true, "", new { @class = "text-danger" })

@using (Html.BeginForm("ChangeEmail", "Account", FormMethod.Post))
{
    <input type="text" name="email" />
    <input type="submit" value=@LangUs.Submit />
}